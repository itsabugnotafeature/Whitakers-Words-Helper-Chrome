{
  "name": "Whitaker's Words Helper",
  "short_name": "WWH",
  "version": "1.1",
  "description": "This extension uses the wonderful site designed by William Whitaker, to easily get detailed and accurate Latin definitions.",
  "permissions": ["contextMenus", "http://archives.nd.edu/"],
  "background": {
    "scripts": ["jQuery.js", "background.js", "contextMenu.js"],
    "persistent": false
  },
  "content_scripts" : [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["jQuery.js", "script.js"],
      "css": ["style.css"]
    }
  ],
  "icons": {
    "16": "images/wwh16.png",
    "32": "images/wwh32.png",
    "48": "images/wwh48.png",
    "128": "images/wwh128.png"
  },
  "manifest_version": 2
}
